#include<iostream>
#include<stdio.h>
using namespace std;
int gcd(int a, int b){
	while(b>0){
		int t = a%b;
		a = b;
		b = t;
	}
	return a;
}
int main(){
	int n,m;
	cin>>n>>m;
	long long int lcm = 1;
	int hcf; 
	for(int i=0;i<n;i++){
		int x;
		cin>>x;
		lcm = lcm*x;
		if(i == 0)
			hcf = gcd(x,x);
		else
			hcf = gcd(hcf,x);
	}
	lcm = lcm/hcf;
	for(int i=0;i<m;i++){
		int x;
		cin>>x;
		if(i == 0)
			hcf = gcd(x,x);
		else
			hcf = gcd(hcf,x);
		
	}
	int ans = 0;
	if(hcf%lcm == 0){
		int i = 1;
		while(lcm*i < hcf ){
			if(hcf%(lcm*i))ans++;
			i += 1;
		}	
	}
	cout<<ans<<endl;
	
}
